{
  "compilerOptions": {
    "declaration": true,
    "outDir": "dist",
    "target": "es5",
    "sourceMap": true,
    "removeComments": true,
    "importHelpers": true,
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/index.ts"]
}
